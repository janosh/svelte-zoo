{
  "name": "svelte-zoo",
  "description": "Random assortment of Svelte components for doc sites",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-zoo",
  "repository": "https://github.com/janosh/svelte-zoo",
  "license": "MIT",
  "version": "0.4.18",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-zoo/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "check": "svelte-check --ignore dist",
    "test": "vitest"
  },
  "dependencies": {
    "@sveltejs/kit": "^2.21.4",
    "highlight.js": "^11.11.1",
    "svelte": "5.33.18",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^4.4.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.28.0",
    "eslint-plugin-svelte": "^3.9.2",
    "hastscript": "^9.0.1",
    "jsdom": "^26.1.0",
    "mdsvex": "^0.12.6",
    "mdsvexamples": "^0.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "svelte-check": "^4.2.1",
    "svelte-multiselect": "11.1.1",
    "svelte-preprocess": "^6.0.3",
    "svelte2tsx": "^0.7.39",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "keywords": [
    "svelte",
    "doc site components",
    "component library",
    "svelte actions",
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./CodeExample.svelte": {
      "types": "./dist/CodeExample.svelte.d.ts",
      "svelte": "./dist/CodeExample.svelte",
      "default": "./dist/CodeExample.svelte"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "default": "./dist/actions.js"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "default": "./dist/stores.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "overrides": {
    "mdsvexamples": {
      "svelte": "$svelte"
    }
  }
}
