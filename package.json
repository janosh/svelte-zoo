{
  "name": "svelte-zoo",
  "description": "Random assortment of Svelte components for doc sites",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-zoo",
  "repository": "https://github.com/janosh/svelte-zoo",
  "license": "MIT",
  "version": "0.3.2",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-zoo/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "check": "svelte-check --ignore package",
    "test": "vitest"
  },
  "dependencies": {
    "svelte": "^3.55.1",
    "@sveltejs/kit": "^1.7.2"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/package": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "eslint": "^8.34.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "hastscript": "^7.2.0",
    "jsdom": "^21.1.0",
    "mdsvex": "^0.10.6",
    "mdsvexamples": "^0.3.3",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte-check": "^3.0.3",
    "svelte-preprocess": "^5.0.1",
    "svelte2tsx": "^0.6.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vitest": "^0.28.5"
  },
  "keywords": [
    "svelte",
    "doc site components",
    "component library",
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "default": "./dist/actions.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "svelte": "./dist/icons/index.js",
      "default": "./dist/icons/index.js"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "default": "./dist/stores.js"
    }
  },
  "files": [
    "dist"
  ]
}
