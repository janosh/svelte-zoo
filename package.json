{
  "name": "svelte-zoo",
  "description": "Random assortment of Svelte components for doc sites",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-zoo",
  "repository": "https://github.com/janosh/svelte-zoo",
  "license": "MIT",
  "version": "0.4.11",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-zoo/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "check": "svelte-check --ignore dist",
    "test": "vitest"
  },
  "dependencies": {
    "@sveltejs/kit": "^2.5.22",
    "highlight.js": "^11.10.0",
    "svelte": "4.2.18"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "eslint-plugin-svelte": "^2.43.0",
    "hastscript": "^9.0.0",
    "jsdom": "^24.1.1",
    "mdsvex": "^0.12.3",
    "mdsvexamples": "^0.4.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "svelte-check": "^3.8.5",
    "svelte-multiselect": "^10.3.0",
    "svelte-preprocess": "^6.0.2",
    "svelte2tsx": "^0.7.15",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "keywords": [
    "svelte",
    "doc site components",
    "component library",
    "svelte actions",
    "icons",
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./CodeExample.svelte": {
      "types": "./dist/CodeExample.svelte.d.ts",
      "svelte": "./dist/CodeExample.svelte",
      "default": "./dist/CodeExample.svelte"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "default": "./dist/actions.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "svelte": "./dist/icons/index.js",
      "default": "./dist/icons/index.js"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "default": "./dist/stores.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
