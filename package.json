{
  "name": "svelte-zoo",
  "description": "Random assortment of Svelte components for doc sites",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-zoo",
  "repository": "https://github.com/janosh/svelte-zoo",
  "license": "MIT",
  "version": "0.4.13",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-zoo/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "check": "svelte-check --ignore dist",
    "test": "vitest"
  },
  "dependencies": {
    "@sveltejs/kit": "^2.6.1",
    "highlight.js": "^11.10.0",
    "svelte": "4.2.19",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.11.1",
    "eslint-plugin-svelte": "^2.44.1",
    "hastscript": "^9.0.0",
    "jsdom": "^25.0.1",
    "mdsvex": "^0.12.3",
    "mdsvexamples": "^0.4.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "svelte-check": "^4.0.4",
    "svelte-multiselect": "^10.3.0",
    "svelte-preprocess": "^6.0.3",
    "svelte2tsx": "^0.7.21",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  },
  "keywords": [
    "svelte",
    "doc site components",
    "component library",
    "svelte actions",
    "icons",
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./CodeExample.svelte": {
      "types": "./dist/CodeExample.svelte.d.ts",
      "svelte": "./dist/CodeExample.svelte",
      "default": "./dist/CodeExample.svelte"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "default": "./dist/actions.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "svelte": "./dist/icons/index.js",
      "default": "./dist/icons/index.js"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "default": "./dist/stores.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ]
}
