{
  "name": "svelte-zoo",
  "description": "Random assortment of Svelte components for doc sites",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.io/svelte-zoo",
  "repository": "https://github.com/janosh/svelte-zoo",
  "license": "MIT",
  "version": "0.3.4",
  "type": "module",
  "svelte": "./dist/index.js",
  "bugs": "https://github.com/janosh/svelte-zoo/issues",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "check": "svelte-check --ignore dist",
    "test": "vitest"
  },
  "dependencies": {
    "@sveltejs/kit": "^1.15.7",
    "svelte": "^3.58.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "hastscript": "^7.2.0",
    "jsdom": "^21.1.1",
    "mdsvex": "^0.10.6",
    "mdsvexamples": "^0.3.3",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "svelte2tsx": "^0.6.11",
    "typescript": "5.0.4",
    "vite": "^4.3.0",
    "vitest": "^0.30.1"
  },
  "keywords": [
    "svelte",
    "doc site components",
    "component library",
    "svelte actions",
    "icons",
    "sveltekit"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./CodeExample.svelte": {
      "types": "./dist/CodeExample.svelte.d.ts",
      "svelte": "./dist/CodeExample.svelte",
      "default": "./dist/CodeExample.svelte"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "default": "./dist/actions.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "svelte": "./dist/icons/index.js",
      "default": "./dist/icons/index.js"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "default": "./dist/stores.js"
    }
  },
  "files": [
    "dist"
  ]
}
